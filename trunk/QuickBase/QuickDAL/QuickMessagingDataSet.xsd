            <Sources>
              <DbSource ConnectionRef="Quick_ERPConnectionString (MySettings)" DbObjectName="Quick_ERP.dbo.Messaging_MessageGroup" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="True" GeneratorGetMethodName="GetAllWithStatus" GeneratorSourceName="FillBy" GetMethodModifier="Public" GetMethodName="GetAllWithStatus" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="True" UserGetMethodName="GetAllWithStatus" UserSourceName="GetAllWithStatus1">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT     Messaging_MessageGroup.Co_ID, Messaging_MessageGroup.MessageGroupID, Messaging_MessageGroup.MessageGroupName, 
                      Messaging_MessageGroup.MessageGroupDesc, Messaging_MessageGroup.DailyMessageLimit, Messaging_MessageGroup.RecordStatusID, 
                      Messaging_MessageGroup.InactiveFrom, Messaging_MessageGroup.InactiveTo, Messaging_MessageGroup.Stamp_DateTime, 
                      Messaging_MessageGroup.Stamp_UserID, Messaging_MessageGroup.Upload_DateTime, COUNT(mgua.Co_ID) AS MembersCount, CONVERT(BIT, COUNT(ustatus.Co_ID)) AS IsMember

FROM         Messaging_MessageGroup 
	LEFT OUTER JOIN
                      Messaging_MessageGroupUserAssociation AS ustatus ON Messaging_MessageGroup.Co_ID = ustatus.Co_ID AND 
                      Messaging_MessageGroup.MessageGroupID = ustatus.MessageGroupID AND ustatus.UserID = @UserID 
	LEFT OUTER JOIN
                      Messaging_MessageGroupUserAssociation AS mgua ON Messaging_MessageGroup.Co_ID = mgua.Co_ID AND 
                      Messaging_MessageGroup.MessageGroupID = mgua.MessageGroupID

WHERE Messaging_MessageGroup.Co_ID = @CoID

GROUP BY Messaging_MessageGroup.Co_ID, Messaging_MessageGroup.MessageGroupID, Messaging_MessageGroup.MessageGroupName, 
                      Messaging_MessageGroup.MessageGroupDesc, Messaging_MessageGroup.DailyMessageLimit, Messaging_MessageGroup.RecordStatusID, 
                      Messaging_MessageGroup.InactiveFrom, Messaging_MessageGroup.InactiveTo, Messaging_MessageGroup.Stamp_DateTime, 
                      Messaging_MessageGroup.Stamp_UserID, Messaging_MessageGroup.Upload_DateTime

